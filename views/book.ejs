            <div><a href="help.html">Help</a></div>
            <div><a href="">Profile</a></div>
        </nav>
    </header>

     <!-- from -->
     <div id="form">
        
        <form action="">
            <button class="radio">
                <label for="round">
                <input type="radio" class="form-check-input" name="trip" id="round" value="round">  
                Round trip </label>
            </button>
            <button class="radio">
                <label for="one">
                <input type="radio" class="form-check-input" name="trip" id="one" value="one">  
                One way</label><br><br>
            </button>
            <br>
            <input type="text" id="inputEmail" class="form-control" aria-describedby="readonly input example" placeholder="From" name="">
            <input type="text" id="inputEmail" class="form-control" aria-describedby="readonly input example" placeholder="Where to?" name="">
            <!-- <input type="date" id="inputEmail" class="form-control"  name="">
            <input type="date" id="inputEmail" class="form-control"  name=""> -->
            <input type="date" placeholder="dd-mm-yy" class="form-control date d">
            <input type="date" placeholder="dd-mm-yy" class="form-control date" id="returndate">

            <select class="form-select" aria-label="Default select example">
                <option selected>Open this select menu</option>
                <option value="1">One</option>
                <option value="2">Two</option>
                <option value="3">Three</option>
            </select>
            <button type="button" class="btn btn-primary">Submit</button>
        </form>
    </div>

    <!-- main -->
    <main>
        <div class="main">
            <% for(Detail of d) {%>
            <div id="detial">
                <div class="part1">
                    <h6>Departure | Mon, Jun 17</h6>
                    <div id="big-box">
                        
                        <div>
                            <img src="<%= Detail.img %>" alt="logo" width="50px" height="50px%">  
                            <div id="airline"><%= Detail.flightname %><br>Flight No : <%= Detail.flightno %> | EQP</div> 
                        </div>
                        <div class="loc loc1">
                                <p><%= Detail.start %><br><%= Detail.from %></p>
                                <span class="material-symbols-outlined">
                                location_on
                                </span>
                        </div>
                        <div id="line">
                            <%= Detail.totaltime %><br>
                        </div>
                        <div class="loc">
                            <span class="material-symbols-outlined">
                            location_on
                            </span>
                            <p><%= Detail.end %><br><%= Detail.to %></p>
                        </div>
                    </div>
                    <hr>
                    <h6>Return | Mon, Jun 24</h6>
                    <div id="big-box">
                        <div>
                            <img src="<%= Detail.img%>" alt="logo" width="50px" height="50px">  
                            <div id="airline"><%= Detail.flightname %><br>Flight No : <%= Detail.flightno %> | EQP</div> 
                        </div>
                            <div class="loc loc1">
                                <p><%= Detail.start %><br><%= Detail.to %></p>
                                <span class="material-symbols-outlined">
                                    location_on
                                    </span>
                            </div>
                            <div id="line">
                                <%= Detail.totaltime %><br>
                            </div>
                            <div class="loc">
                                <span class="material-symbols-outlined">
                                    location_on
                                    </span>
                                    <p><%= Detail.end %><br><%= Detail.from %></p>
                            </div>
                    </div>
                </div>
                <div id="part2">
                    <h5>$203.20</h5>
                    <h6>Fare Detail <br>
                    (incl. taxes)</h6>
                    <button type="button" class="btn btn-danger">Submit</button>
                    <p>Hurry!! Only 9 Seats left</p>
                </div>
               
        </div>
        <% } %>
    </div>
    <aside>
        <div class="aside-box">
            <h5><b>Stop</b></h5>
            <hr>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                <label class="form-check-label" for="flexCheckDefault">
                  Non Stop
                </label>
            </div>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
                <label class="form-check-label" for="flexCheckChecked">
                    One Stop
                </label>
            </div>
        </div>
        <div class="aside-box">
            <h5><b>Airports</b></h4><hr>
            <h5>Departure</h5>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
                <label class="form-check-label" for="flexCheckChecked">
                  One Stop
                </label>
            </div>
            <hr>
            <h5>Return</h5>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
                <label class="form-check-label" for="flexCheckChecked">
                  One Stop
                </label>
            </div>

        </div>
        <div class="aside-box ">
            <h5><b>Airlines</b></h1><hr>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
                <label class="form-check-label" for="flexCheckChecked">
                    One Stop
                </label>
            </div>
        </div>
    </aside>
    </main>
    

